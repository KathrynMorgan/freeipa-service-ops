## Make Root CA Available for DL (On IPA Server)
yum install -y vsftpd
systemctl enable vsftpd
systemctl start vsftpd
cp /etc/ipa/ca.crt /var/ftp/ipa-ca.crt

## Install IPA CA Root Certificate (On Client)
mkdir /usr/local/share/ca-certificates/ipa
wget -P /usr/local/share/ca-certificates/ipa/ ftp://ipa.chrisjohnston.org/ipa-ca.crt
chmod 400 /usr/local/share/ca-certificates/ipa/*
update-ca-certificates
